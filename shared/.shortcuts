--1
dots=cd ~/.dots
tpad=cd ~/.dots/thinkpad
dtop=cd ~/.dots/desktop
mba=cd ~/.dots/mba
srd=cd ~/.dots/shared
music=bash ~/.pyscripts/musicscrape/run.sh
assignments=cd ~/school/assignments
gpus=git -C ~/.dots add --all && git -C ~/.dots commit && git -C ~/.dots push
gupd=git -C ~/.dots pull
dgit=git -C ~/.dots
assgen=runhaskell ~/.hscripts/assgen.hs
startx=dbus-run-session startx
sysupd=paru -Syu --noconfirm --sudoloop --skipreview --devel
--2
xr=~/.Xresources
sxrc=~/.config/sxhkd/sxhkdrc
bsprc=~/.config/bspwm/bspwmrc
pbar=~/.config/polybar/config
rng=~/.config/ranger/rc.conf
vrc=~/.config/nvim/init.lua
zrc=~/.zshrc
src=~/.shortcuts
srcs=~/.shortcuts_specific
