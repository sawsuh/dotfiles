--Commands
dots: cd ~/.dots
y: yay
home: cd ~
fact: wget -qO- randomfunfacts.com | sed -n "/<strong>/{s;^.*<i>\(.*\)</i>.*$;\1;p}"
music: bash ~/.pyscripts/musicscrape/run.sh
dfu: bash ~/.scripts/dfu.sh 
alarm: bash ~/.scripts/alarm.sh
yi: yay -Slq | fzf -m --preview "yay -Si {1}" | xargs -ro yay -S
gaming: bash ~/.scripts/gaming.sh
onemon: bash ~/.scripts/onemon.sh
twomon: bash ~/.scripts/twomon.sh
assignments: cd ~/school/assignments
--FilestoVim
xr: ~/.Xresources
sxrc: ~/.config/sxhkd/sxhkdrc
bsprc: ~/.config/bspwm/bspwmrc
pbar: ~/.config/polybar/config
rng: ~/.config/ranger/rc.conf
vrc: ~/.vimrc
